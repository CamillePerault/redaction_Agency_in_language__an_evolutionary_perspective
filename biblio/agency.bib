Publisher: American Psychological Association},
	keywords = {*Psychological Theories, *Thinking, Abstraction, Egocentrism},
}

@book{twenge_generation_2006,
	location = {New York,  {NY},  {US}},
	title = {Generation Me: Why today's young Americans are more confident, assertive, entitled--and more miserable than ever before.},
	isbn = {0-7432-7697-3 (Hardcover); 978-0-7432-7697-9 (Hardcover)},
	series = {Generation Me: Why today's young Americans are more confident, assertive, entitled--and more miserable than ever before.},
	abstract = {In this provocative new book, headline-making psychologist and social commentator Dr. Jean Twenge documents the self-focus of what she calls "Generation Me"--people born in the 1970s, 1980s, and 1990s. Herself a member of Generation Me, Dr. Twenge explores why her generation is tolerant, confident, open-minded, and ambitious but also cynical, depressed, lonely, and anxious. Using findings from the largest intergenerational study ever conducted--with data from 1.3 million respondents spanning six decades--Dr. Twenge reveals how profoundly different today's young adults are--and makes controversial predictions about what the future holds for them and society as a whole. But Dr. Twenge doesn't just talk statistics--she highlights real-life people and stories and vividly brings to life the hopes and dreams, disappointments and challenges of Generation Me. With a good deal of irony, humor, and sympathy she demonstrates that today's young people have been raised to aim for the stars at a time when it is more difficult than ever to get into college, find a good job, and afford a house--even with two incomes. {GenMe}'s expectations have been raised just as the world is becoming more competitive, creating an enormous clash between expectations and reality. Dr. Twenge also presents the often-shocking truths about her generation's dramatically different sexual behavior and mores. {GenMe} has created a profound shift in the American character, changing what it means to be an individual in today's society. ({PsycINFO} Database Record (c) 2016 {APA}, all rights reserved)},
	pagetotal = {vii, 292},
	publisher = {Free Press},
